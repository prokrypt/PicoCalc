+void ResetController(void) {
+    PinSetBit(Pico_LCD_RST, LATSET);
+    sleep_us(10000);
+    PinSetBit(Pico_LCD_RST, LATCLR);
+    sleep_us(10000);
+    PinSetBit(Pico_LCD_RST, LATSET);
+    sleep_us(200000);
+}
